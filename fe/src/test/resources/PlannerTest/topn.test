select int_col, sum(float_col) 
from hbasealltypessmall
where id < 5
group by 1
order by 2
limit 4
----
TOP-N
ORDER BY: <slot 4> ASC
LIMIT: 4
  AGGREGATE
  OUTPUT: SUM(float_col)
  GROUP BY: int_col
    SCAN HBASE table=hbasealltypessmall
      PREDICATES: id < 5
----
TOP-N
ORDER BY: <slot 4> ASC
LIMIT: 4
  AGGREGATE
  OUTPUT: SUM(<slot 4>)
  GROUP BY: <slot 3>
    EXCHANGE ID: 2
----
AGGREGATE
OUTPUT: SUM(float_col)
GROUP BY: int_col
LIMIT: 4
  SCAN HBASE table=hbasealltypessmall
    PREDICATES: id < 5
====
select int_col, sum(float_col), min(float_col) 
from hbasealltypessmall
group by 1
order by 2,3 desc
limit 0
----
TOP-N
ORDER BY: <slot 3> ASC, <slot 4> DESC
LIMIT: 0
  AGGREGATE
  OUTPUT: SUM(float_col), MIN(float_col)
  GROUP BY: int_col
    SCAN HBASE table=hbasealltypessmall
----
TOP-N
ORDER BY: <slot 3> ASC, <slot 4> DESC
LIMIT: 0
  AGGREGATE
  OUTPUT: SUM(<slot 3>), MIN(<slot 4>)
  GROUP BY: <slot 2>
    EXCHANGE ID: 2
----
AGGREGATE
OUTPUT: SUM(float_col), MIN(float_col)
GROUP BY: int_col
LIMIT: 0
  SCAN HBASE table=hbasealltypessmall
====
