select name, zip
from testtbl
order by name
limit 1
----
TOP-N
ORDER BY: name ASC
LIMIT: 1
  SCAN HDFS table=default.testtbl
    FILES:
----
TOP-N
ORDER BY: name ASC
LIMIT: 1
  EXCHANGE ID: 1
----
SCAN HDFS table=default.testtbl
  FILES:
  LIMIT: 1
====
select zip, count(*)
from testtbl
where name like 'm%'
group by 1
order by 2 desc
limit 10
----
TOP-N
ORDER BY: <slot 3> DESC
LIMIT: 10
  AGGREGATE
  OUTPUT: COUNT(*)
  GROUP BY: zip
    SCAN HDFS table=default.testtbl
      PREDICATES: name LIKE 'm%'
      FILES:
----
TOP-N
ORDER BY: <slot 3> DESC
LIMIT: 10
  AGGREGATE
  OUTPUT: SUM(<slot 3>)
  GROUP BY: <slot 2>
    EXCHANGE ID: 2
----
AGGREGATE
OUTPUT: COUNT(*)
GROUP BY: zip
LIMIT: 10
  SCAN HDFS table=default.testtbl
    PREDICATES: name LIKE 'm%'
    FILES:
