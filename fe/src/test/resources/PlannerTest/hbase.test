# with aggregation
select int_col, count(*) 
from hbasealltypessmall
where id < 5
group by 1
---- PLAN
Plan Fragment 0
  AGGREGATE
  OUTPUT: COUNT(*)
  GROUP BY: int_col
    SCAN HBASE table=hbasealltypessmall (0)
      PREDICATES: id < 5

  EXEC PARAMS
    HOST: 
      HBASE KEY RANGES NODE ID: 0
        <unbounded>:<unbounded>
------------ DISTRIBUTEDPLAN
Plan Fragment 0
  AGGREGATE
  OUTPUT: SUM(<slot 3>)
  GROUP BY: <slot 2>
    EXCHANGE (2)

Plan Fragment 1
  STREAM DATA SINK
    EXCHANGE ID: 2
  AGGREGATE
  OUTPUT: COUNT(*)
  GROUP BY: int_col
    SCAN HBASE table=hbasealltypessmall (0)
      PREDICATES: id < 5

  EXEC PARAMS
    HOST: 
      HBASE KEY RANGES NODE ID: 0
        <unbounded>:<unbounded>
