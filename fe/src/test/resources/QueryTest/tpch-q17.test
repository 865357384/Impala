# Q17 - Small-Quantity-Order Revenue Query
insert overwrite table lineitem_tmp$TABLE
select l_partkey as t_partkey, 0.2 * avg(l_quantity) as t_avg_quantity
from lineitem$TABLE
group by l_partkey
---- SETUP
RESET lineitem_tmp$TABLE
RELOAD lineitem_tmp$TABLE
