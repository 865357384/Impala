# QUERY_NAME : TPCH-Q15_QUERY_2
insert overwrite table tpch.max_revenue
select max(total_revenue)
from tpch.revenue
---- SETUP
RESET tpch.max_revenue
RELOAD tpch.max_revenue
---- RESULTS
---- PARTITIONS
/: 1
====
# QUERY_NAME : TPCH-Q15_QUERY_3
# Modifications - Added limit
select
  s_suppkey,
  s_name,
  s_address,
  s_phone,
  total_revenue
from tpch.supplier$TABLE s
join tpch.revenue r
  on (s.s_suppkey = r.supplier_no)
join tpch.max_revenue m
  on (r.total_revenue = m.max_revenue)
order by s_suppkey
limit 100
