  l_orderkey,
  round(sum(l_extendedprice * (1 - l_discount)), 5) as revenue,
  o_orderdate,
  o_shippriority
from tpch.lineitem l
  join tpch.orders o
    on (l.l_orderkey = o.o_orderkey)
  join tpch.customer c
