====
---- QUERY : TPCDS-Q42
SELECT
  d.d_year,
  i.i_category_id,
  i.i_category,
  SUM(ss_ext_sales_price) total_sales
FROM store_sales ss
JOIN date_dim d
  ON (ss.ss_sold_date_sk = d.d_date_sk)
JOIN item i
  ON (ss.ss_item_sk = i.i_item_sk)
WHERE
  i.i_manager_id = 1
  AND d.d_moy = 12
  AND d.d_year = 1998
GROUP BY
  d.d_year,
  i.i_category_id,
  i.i_category
ORDER BY
  total_sales DESC,
  d_year,
  i_category_id,
  i_category
LIMIT 100
