---- QUERY
# aggregate with group-by, having
select *
from ( 
       select int_col % 7 c1, count(*) c2, avg(int_col) c3
       from (
              select * from alltypesagg$TABLE
            ) a
       group by 1
       having avg(int_col) > 500 or count(*) = 10
     ) b
where c1 is not null
and   c2 > 10
---- TYPES
int, bigint, double
